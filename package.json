{
	"name": "antigravity-bridge",
	"version": "0.1.0",
	"description": "Discord と Antigravity を繋ぐエージェント・ブリッジ",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"dev": "tsx watch src/index.ts",
		"deploy-commands": "tsx src/deploy-commands.ts",
		"lint": "biome check",
		"lint:fix": "biome check --write",
		"format": "biome format --write",
		"prepare": "husky"
	},
	"dependencies": {
		"discord.js": "^14.18.0",
		"dotenv": "^16.4.7"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.2",
		"@types/node": "^22.13.4",
		"husky": "^9.1.7",
		"tsx": "^4.19.3",
		"typescript": "^5.7.3"
	}
}
